{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "pre-commit": "npm run code-style-check",
    "code-style-check": "npm run prettier && npm run lint",
    "auto-fix": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "start": "node ./src/index",
    "dev": "NODE_ENV=development nodemon ./src/index",
    "build": "NODE_ENV=production node ./src/index",
    "setup": "npm install --include=dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "eslint": "^9.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2"
  }
}
